# Add BASH aliases
alias cjs="code . && jekyll serve"
alias ghp="git checkout gh-pages"
alias safari=""

alias cmp-c="cd ~/Code/cmp.arteveldehogeschool.local"
alias cmp-s="cd ~/Syllabi/cmp && open http://localhost:4000/cmp/ && cjs"
alias ed-c="cd ~/Code/ed.arteveldehogeschool.local"
alias ed-s="cd ~/Syllabi/cmp && cjs"
alias nmdad2-c="cd ~/Code/nmdad2.arteveldehogeschool.local"
alias nmdad2-s="cd ~/Syllabi/nmdad2 && open http://localhost:4000/nmdad2/ && cjs"
alias nmdad3-c="cd ~/Code/nmdad3.arteveldehogeschool.local"
alias nmdad3-s="cd ~/Syllabi/nmdad3 && open http://localhost:4000/nmdad3/ && cjs"
alias wd1-c="cd ~/Code/wd1.arteveldehogeschool.local"
alias wd2-c="cd ~/Code/wd2.arteveldehogeschool.local"