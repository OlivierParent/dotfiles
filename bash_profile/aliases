# Add BASH aliases
alias js="jekyll serve"
alias cjs="code . && js"
alias console="php app/console"
alias ghp="git checkout gh-pages"

alias cmp="cd ~/Code/cmp.arteveldehogeschool.local"
alias cmp-s="cd ~/Syllabi/cmp && open http://localhost:4000/cmp/ && cjs"
alias ed="cd ~/Code/ed.arteveldehogeschool.local"
alias ed-s="cd ~/Syllabi/cmp && cjs"
alias nmdad2="cd ~/Code/nmdad2.arteveldehogeschool.local"
alias nmdad2-s="cd ~/Syllabi/nmdad2 && open http://localhost:4000/nmdad2/ && cjs"
alias nmdad3="cd ~/Code/nmdad3.arteveldehogeschool.local"
alias nmdad3-s="cd ~/Syllabi/nmdad3 && open http://localhost:4000/nmdad3/ && cjs"
alias wd1="cd ~/Code/wd1.arteveldehogeschool.local"
alias wd2="cd ~/Code/wd2.arteveldehogeschool.local"